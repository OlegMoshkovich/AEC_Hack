@prefix dbr: <https://dbpedia.org/resource/> .

<http://example.com/buildings/BB> a bot:Building , schema:Place , rec:Building ;
    schema:name "Ulrikkenborg Plads 1" ;
    schema:image <https://emoweb.dk/EMODigital/EMODigital.svc/Picture/311439466> ;
    schema:address <http://example.com/buildings/BB/address> ;
    schema:geo <http://example.com/buildings/BB/geo> ;
    se:hasEnergyLabel sev:EnergyLabelC ;
    se:hasEnergySavingsProposal <http://example.com/buildings/BB/ESP1> , 
        <http://example.com/buildings/BB/ESP2> ,
        <http://example.com/buildings/BB/ESP3> ,
        <http://example.com/buildings/BB/ESP4> .

<http://example.com/buildings/BB/address> a schema:PostalAddress ;
    schema:addressCountry dbr:Denmark ;
    schema:postalCode "2800" ;
    schema:streetAddress "Ulrikkenborg Plads 1" .

<http://example.com/buildings/BB/geo>
    schema:latitude 55.7676765 ;
    schema:longitude 12.4998316 .

